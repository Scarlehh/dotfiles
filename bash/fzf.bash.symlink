# Setup fzf
# ---------
if [[ ! "$PATH" == */home/scarlett/.fzf/bin* ]]; then
  export PATH="$PATH:/home/scarlett/.fzf/bin"
fi

# Auto-completion
# ---------------
[[ $- == *i* ]] && source "/home/scarlett/.fzf/shell/completion.bash" 2> /dev/null

# Key bindings
# ------------
source "/home/scarlett/.fzf/shell/key-bindings.bash"

